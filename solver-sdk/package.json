{
  "name": "@velox/solver-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Velox solvers on Movement",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev -r dotenv/config --respawn src/examples/basic-solver.ts",
    "dev:advanced": "ts-node-dev -r dotenv/config --respawn src/examples/advanced-solver.ts",
    "dev:dutch": "ts-node-dev -r dotenv/config --respawn src/examples/dutch-solver.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "velox",
    "movement",
    "aptos",
    "dex",
    "solver",
    "intent"
  ],
  "author": "Velox Team",
  "license": "MIT",
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.33.0",
    "dotenv": "^16.6.1",
    "got": "^14.6.5",
    "graphql": "^16.12.0",
    "graphql-request": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
